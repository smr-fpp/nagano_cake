

  <div class="container">
    <p>ジャンル編集</p>
    <div class ="row">
      
      <%= form_with model: @genre ,url: admin_genre_path(@genre.id), method: :patch do |f| %>
      <div class="form-group">
        <%= f.label :ジャンル名, class: "mr-5" %>
        <%= f.text_field :name, class: "mr-5" %>
        <%= f.submit '変更を保存', class: "btn btn-success" %>
      </div>
      <% end %>
    </div>
    

  </div>
    
  

